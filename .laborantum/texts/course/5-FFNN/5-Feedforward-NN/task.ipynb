    "        self.fc2 = torch.nn.Linear(n_hidden_neurons, 10)\n",
    "        \n",
    "    \n",
    "    def __call__(self, signal):\n",
    "\n",
    "        ## YOUR CODE HERE\n",
    "        signal = self.fc1(signal)\n",
    "        signal = self.ac1(signal)\n",
    "        signal = self.fc2(signal)\n",
    "\n",
    "\n",
    "        return signal"
